<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".detallereclamo.DetalleReclamoInsertarActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/detalle_id"
        android:id="@+id/textView49"
        android:layout_above="@+id/textView50"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <EditText
        android:id="@+id/editDetalle_id"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:maxLength="7"
        android:layout_marginTop="34dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView50"
        android:layout_toEndOf="@+id/textView50" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/idProdServ"
        android:id="@+id/textView50"
        android:layout_marginTop="56dp"
        android:layout_below="@+id/editDetalle_id"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <EditText
        android:id="@+id/idProdServ"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:maxLength="7"
        android:layout_alignBottom="@+id/textView50"
        android:layout_toEndOf="@+id/insertarEstado"

        android:layout_toRightOf="@+id/insertarEstado"
        android:layout_alignLeft="@+id/editDescripcionDetalle"
        android:layout_alignStart="@+id/editDescripcionDetalle" />

    <TextView android:text="@string/detalle_Descripcion" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView28"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/textView49"
        android:layout_alignStart="@+id/textView49" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/editDescripcionDetalle"
        android:layout_alignRight="@+id/editDetalle_id"
        android:layout_alignEnd="@+id/editDetalle_id"
        android:layout_alignBottom="@+id/textView28"
        android:layout_toRightOf="@+id/textView50"
        android:layout_toEndOf="@+id/textView50"
        android:layout_below="@+id/idProdServ" />



    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/insertar_boton"
        android:id="@+id/insertarEstado"
        android:onClick="insertarEstado"
        android:layout_alignTop="@+id/button16"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/limpiar_boton"
        android:id="@+id/button16"
        android:onClick="limpiarTexto"
        android:layout_below="@+id/editDescripcionDetalle"
        android:layout_alignRight="@+id/editDescripcionDetalle"
        android:layout_alignEnd="@+id/editDescripcionDetalle"
        android:layout_marginTop="78dp" />


    <Button
        android:id="@+id/mainbttomarfotodetarecl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/adjuntarfoto"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/button16"
        android:layout_toLeftOf="@+id/button16" />
</RelativeLayout>