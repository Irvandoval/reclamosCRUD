<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.irvandoval.reclamosgrupo17.categoriaempresa.CategoriaEmpresaInsertarActvity">

    <TextView android:text="@string/nombrecatempresa" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:id="@+id/textView31"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="225dp"
        android:layout_height="wrap_content"
        android:id="@+id/NombreCE"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/textView31"
        android:layout_alignLeft="@+id/DescripcionCE"
        android:layout_alignStart="@+id/DescripcionCE" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/descripcioncatempresa"
        android:id="@+id/textView33"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/NombreCE"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/DescripcionCE"
        android:layout_below="@+id/textView33"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="27dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/insertar_boton"
        android:id="@+id/button17"
        android:layout_below="@+id/DescripcionCE"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="31dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="36dp"
        android:onClick="insertarCatEmpresa" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/limpiar_boton"
        android:id="@+id/button18"
        android:layout_alignBottom="@+id/button17"
        android:layout_alignRight="@+id/NombreCE"
        android:layout_alignEnd="@+id/NombreCE"
        android:onClick="limpiarCE" />

</RelativeLayout>
