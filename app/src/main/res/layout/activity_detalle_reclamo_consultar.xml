<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.irvandoval.reclamosgrupo17.detallereclamo.DetalleReclamoConsultarActivity"
    android:id="@+id/icon">

    <TextView android:text="@string/detalle_Descripcion" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView28"
        android:layout_marginTop="106dp"
        android:layout_below="@+id/button15"
        android:layout_alignLeft="@+id/button15"
        android:layout_alignStart="@+id/button15" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/editDescripcionDetalle"
        android:layout_alignBottom="@+id/textView28"
        android:layout_alignRight="@+id/editDetalle_id"
        android:layout_alignEnd="@+id/editDetalle_id"
        android:layout_alignLeft="@+id/editDetalle_id"
        android:layout_alignStart="@+id/editDetalle_id" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/detalle_id"
        android:id="@+id/textView"
        android:layout_marginTop="36dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editDetalle_id"
        android:layout_alignBottom="@+id/textView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/consultar_boton"
        android:id="@+id/button15"
        android:onClick="consultarDetalle"
        android:layout_marginTop="94dp"
        android:layout_below="@+id/textView"
        android:layout_toLeftOf="@+id/editDescripcionDetalle"
        android:layout_toStartOf="@+id/editDescripcionDetalle" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/limpiar_boton"
        android:id="@+id/button16"
        android:layout_marginRight="44dp"
        android:layout_marginEnd="44dp"
        android:layout_above="@+id/textView28"
        android:layout_alignRight="@+id/editDescripcionDetalle"
        android:layout_alignEnd="@+id/editDescripcionDetalle"
        android:onClick="limpiarTexto"/>

    <TextView
        android:text="@string/idProdServ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView44"
        android:layout_marginTop="41dp"
        android:layout_below="@+id/editDescripcionDetalle"
        android:layout_alignLeft="@+id/textView28"
        android:layout_alignStart="@+id/textView28" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/editProServ"
        android:layout_alignBottom="@+id/textView44"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/button16"
        android:layout_alignStart="@+id/button16" />

</RelativeLayout>