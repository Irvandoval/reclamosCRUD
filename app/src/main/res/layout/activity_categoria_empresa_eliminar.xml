<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.irvandoval.reclamosgrupo17.categoriaempresa.CategoriaEmpresaEliminarActivity">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/idcatempresa"
        android:id="@+id/textView50"
        android:layout_marginTop="39dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/IDCE"
        android:layout_below="@+id/textView50"
        android:layout_centerHorizontal="true" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/eliminar_boton"
        android:id="@+id/button17"
        android:onClick="eliminarCE"
        android:layout_alignTop="@+id/button18"
        android:layout_toLeftOf="@+id/IDCE"
        android:layout_toStartOf="@+id/IDCE" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/limpiar_boton"
        android:id="@+id/button18"
        android:onClick="limpiarCE"
        android:layout_marginTop="42dp"
        android:layout_below="@+id/IDCE"
        android:layout_toRightOf="@+id/IDCE"
        android:layout_toEndOf="@+id/IDCE" />
</RelativeLayout>
