<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.irvandoval.reclamosgrupo17.prodserv.ProdServInsertarActivity">

    <TextView android:text="@string/idProdServ" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="46dp"
        android:id="@+id/textView46"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/idProdServ"
        android:layout_toStartOf="@+id/idProdServ"
        android:layout_marginRight="51dp"
        android:layout_marginEnd="51dp" />

    <EditText
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/idProdServ"
        android:layout_alignBottom="@+id/textView46"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="31dp"
        android:layout_marginEnd="31dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/nombreCatPS"
        android:id="@+id/textView47"
        android:layout_below="@+id/idProdServ"
        android:layout_alignLeft="@+id/textView46"
        android:layout_alignStart="@+id/textView46"
        android:layout_marginTop="28dp" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/nombreProdServ"
        android:layout_alignTop="@+id/textView47"
        android:layout_alignRight="@+id/idProdServ"
        android:layout_alignEnd="@+id/idProdServ"
        android:layout_marginRight="25dp"
        android:layout_marginEnd="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/descripcionCatPS"
        android:id="@+id/textView48"
        android:layout_marginTop="45dp"
        android:layout_below="@+id/nombreProdServ"
        android:layout_alignRight="@+id/textView47"
        android:layout_alignEnd="@+id/textView47" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/descripcionProdServ"
        android:layout_alignTop="@+id/textView48"
        android:layout_alignLeft="@+id/nombreProdServ"
        android:layout_alignStart="@+id/nombreProdServ" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/insertar_boton"
        android:id="@+id/button29"
        android:layout_below="@+id/descripcionProdServ"
        android:layout_alignLeft="@+id/textView48"
        android:layout_alignStart="@+id/textView48"
        android:layout_marginTop="32dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/limpiar_boton"
        android:id="@+id/button30"
        android:layout_alignTop="@+id/button29"
        android:layout_alignRight="@+id/idProdServ"
        android:layout_alignEnd="@+id/idProdServ" />

</RelativeLayout>
