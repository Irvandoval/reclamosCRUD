<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.irvandoval.reclamosgrupo17.estadoreclamo.EstadoReclamoEliminarActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/estadoReclamo_id"
        android:id="@+id/textView49"
        android:layout_alignTop="@+id/editEstadoId"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <EditText
        android:id="@+id/editEstadoId"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:maxLength="7"
        android:layout_marginTop="34dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView49"
        android:layout_toEndOf="@+id/textView49" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/eliminar"
        android:onClick="eliminarEstado"
        android:id="@+id/button31"
        android:layout_marginTop="79dp"
        android:layout_below="@+id/editEstadoId"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
