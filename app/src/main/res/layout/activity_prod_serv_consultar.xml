<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.irvandoval.reclamosgrupo17.prodserv.ProdServConsultarActivity">

    <TextView android:text="@string/idProdServ" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="46dp"
        android:id="@+id/textView46"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/idProdServ"
        android:layout_toStartOf="@+id/idProdServ"
        android:layout_marginRight="51dp"
        android:layout_marginEnd="51dp" />

    <EditText
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/idProdServ"
        android:layout_alignTop="@+id/textView46"
        android:layout_alignLeft="@+id/descripcionProdServ"
        android:layout_alignStart="@+id/descripcionProdServ" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/nombreCatPS"
        android:id="@+id/textView47"
        android:layout_above="@+id/descripcionProdServ"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/nombreProdServ"
        android:layout_alignTop="@+id/textView47"
        android:layout_toRightOf="@+id/textView45"
        android:layout_toEndOf="@+id/textView45" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/descripcionCatPS"
        android:id="@+id/textView48"
        android:layout_alignBaseline="@+id/descripcionProdServ"
        android:layout_alignBottom="@+id/descripcionProdServ"
        android:layout_alignRight="@+id/textView46"
        android:layout_alignEnd="@+id/textView46" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/descripcionProdServ"
        android:layout_above="@+id/button30"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="19dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/limpiar_boton"
        android:id="@+id/button30"
        android:onClick="limpiarTexto4"
        android:layout_alignTop="@+id/btnConsultar"
        android:layout_toRightOf="@+id/textView45"
        android:layout_toEndOf="@+id/textView45" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ID_Categoria_Prod_Serv"
        android:id="@+id/textView45"
        android:layout_above="@+id/descripcionProdServ"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="64dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/IdCatProdServ"
        android:layout_alignBottom="@+id/textView45"
        android:layout_alignLeft="@+id/button30"
        android:layout_alignStart="@+id/button30"
        android:layout_alignRight="@+id/idProdServ"
        android:layout_alignEnd="@+id/idProdServ" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/consultar_boton"
        android:id="@+id/btnConsultar"
        android:onClick="consultarProductoServ"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/descripcionProdServ"
        android:layout_toStartOf="@+id/descripcionProdServ"
        android:layout_marginBottom="28dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton5"
        android:src="@mipmap/button_icon"
        android:onClick="busquedaPorVoz"
        android:layout_above="@+id/IdCatProdServ"
        android:layout_toRightOf="@+id/IdCatProdServ"
        android:layout_toEndOf="@+id/IdCatProdServ" />

</RelativeLayout>
