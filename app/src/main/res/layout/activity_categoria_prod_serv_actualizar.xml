<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.irvandoval.reclamosgrupo17.categoriaprodserv.CategoriaProdServActualizarActivity">

    <TextView android:text="@string/idCategoriaPS" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="43dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="39dp"
        android:id="@+id/textView37" />

    <EditText
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/idCAtPS"
        android:layout_marginLeft="42dp"
        android:layout_marginStart="42dp"
        android:layout_alignBottom="@+id/textView37"
        android:layout_toRightOf="@+id/textView37"
        android:layout_toEndOf="@+id/textView37" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/nomEmp"
        android:id="@+id/textView38"
        android:layout_below="@+id/textView37"
        android:layout_alignLeft="@+id/textView37"
        android:layout_alignStart="@+id/textView37"
        android:layout_marginTop="35dp" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/nombreCatPS"
        android:layout_alignBottom="@+id/textView38"
        android:layout_toRightOf="@+id/textView38"
        android:layout_toEndOf="@+id/textView38"
        android:layout_marginLeft="54dp"
        android:layout_marginStart="54dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/descripcionCatPS"
        android:id="@+id/textView39"
        android:layout_below="@+id/textView38"
        android:layout_alignLeft="@+id/textView38"
        android:layout_alignStart="@+id/textView38"
        android:layout_marginTop="49dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/descripcionCatPS"
        android:layout_alignTop="@+id/textView39"
        android:layout_alignLeft="@+id/nombreCatPS"
        android:layout_alignStart="@+id/nombreCatPS" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/actualizar_boton"
        android:id="@+id/button23"
        android:onClick="actualizarCategoriaProducServ"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/button24"
        android:layout_toStartOf="@+id/button24" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/limpiar_boton"
        android:id="@+id/button24"
        android:onClick="limpiarTexto3"
        android:layout_above="@+id/button23"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/consultar_boton"
        android:id="@+id/button36"
        android:layout_above="@+id/button23"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="consultarCategoriaProductoServ" />

</RelativeLayout>
