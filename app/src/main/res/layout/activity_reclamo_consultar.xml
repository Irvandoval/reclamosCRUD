<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.irvandoval.reclamosgrupo17.reclamo.ReclamoConsularActivity">

    <TextView android:text="@string/Titulo" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:id="@+id/textView8"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/motivo"
        android:id="@+id/motivoReclamo"
        android:layout_marginTop="52dp"
        android:layout_below="@+id/TituloReclamo"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/MotivoReclamo"
        android:layout_below="@+id/TituloReclamo"
        android:layout_toRightOf="@+id/limpiar1"
        android:layout_toEndOf="@+id/limpiar1"
        android:layout_marginTop="26dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/Fecha"
        android:id="@+id/textfec"
        android:layout_marginTop="37dp"
        android:layout_below="@+id/MotivoReclamo"
        android:layout_alignRight="@+id/motivoReclamo"
        android:layout_alignEnd="@+id/motivoReclamo" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Limpiar"
        android:id="@+id/limpiar1"
        android:onClick="limpiarTexto1"
        android:layout_alignLeft="@+id/textView53"
        android:layout_alignStart="@+id/textView53"
        android:layout_below="@+id/idDetalle" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="date"
        android:ems="10"
        android:id="@+id/fechaReclamo"
        android:hint="@string/formatofec"
        android:layout_alignBottom="@+id/textfec"
        android:layout_alignLeft="@+id/MotivoReclamo"
        android:layout_alignStart="@+id/MotivoReclamo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/usuario_dui"
        android:id="@+id/textView52"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/textfec"
        android:layout_alignEnd="@+id/textfec" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/recDui"
        android:layout_alignBottom="@+id/textView52"
        android:layout_alignLeft="@+id/fechaReclamo"
        android:layout_alignStart="@+id/fechaReclamo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/idReclamo"
        android:id="@+id/textView53"
        android:layout_marginTop="28dp"
        android:layout_below="@+id/textView52"
        android:layout_alignRight="@+id/textView52"
        android:layout_alignEnd="@+id/textView52" />

    <EditText
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/idReclamo"
        android:layout_below="@+id/textView53"
        android:layout_alignRight="@+id/textView53"
        android:layout_alignEnd="@+id/textView53" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/sucursal_idsucursal"
        android:id="@+id/textView54"
        android:layout_alignTop="@+id/idSucursal"
        android:layout_toRightOf="@+id/textView55"
        android:layout_toEndOf="@+id/textView55" />

    <EditText
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/idSucursal"
        android:layout_alignTop="@+id/textView53"
        android:layout_alignLeft="@+id/textView54"
        android:layout_alignStart="@+id/textView54" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/detalle_id"
        android:id="@+id/textView55"
        android:layout_below="@+id/idReclamo"
        android:layout_toRightOf="@+id/idReclamo"
        android:layout_toEndOf="@+id/idReclamo" />

    <EditText
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/idDetalle"
        android:layout_below="@+id/textView55"
        android:layout_alignRight="@+id/textView55"
        android:layout_alignEnd="@+id/textView55" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/TituloReclamo"
        android:layout_below="@+id/textView8"
        android:layout_alignLeft="@+id/MotivoReclamo"
        android:layout_alignStart="@+id/MotivoReclamo" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/consultar_boton"
        android:id="@+id/button7"
        android:onClick="consultarReclamo"
        android:layout_below="@+id/idDetalle"
        android:layout_alignRight="@+id/fechaReclamo"
        android:layout_alignEnd="@+id/fechaReclamo" />


</RelativeLayout>
