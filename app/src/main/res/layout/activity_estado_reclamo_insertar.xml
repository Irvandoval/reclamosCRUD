<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".estadoreclamo.EstadoReclamoInsertarActivity">

    <TextView android:text="@string/estadoReclamo_id" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView30"
        android:layout_marginTop="44dp"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/textView29"
        android:layout_alignEnd="@+id/textView29" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/ediEstadoId"
        android:layout_alignTop="@+id/textView30"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_toRightOf="@+id/button15"
        android:layout_toEndOf="@+id/button15" />

    <TextView android:text="@string/estadoReclamo_nombre" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:id="@+id/textView28"
        android:layout_below="@+id/textView30"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/editNombreEstado"
        android:layout_above="@+id/textView29"
        android:layout_alignRight="@+id/ediEstadoId"
        android:layout_alignEnd="@+id/ediEstadoId"
        android:layout_toRightOf="@+id/button15"
        android:layout_toEndOf="@+id/button15" />

    <TextView android:text="@string/estadoReclamo_Descripcion" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:id="@+id/textView29"
        android:layout_below="@+id/textView28"
        android:layout_alignLeft="@+id/textView28"
        android:layout_alignStart="@+id/textView28" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/editDescripcionEstado"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/ediEstadoId"
        android:layout_alignStart="@+id/ediEstadoId"
        android:layout_alignRight="@+id/button16"
        android:layout_alignEnd="@+id/button16" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/insertar"
        android:id="@+id/button15"
        android:layout_alignTop="@+id/button16"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="insertarEstadoReclamo"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/limpiar_boton"
        android:id="@+id/button16"
        android:layout_below="@+id/textView29"
        android:layout_alignRight="@+id/editNombreEstado"
        android:layout_alignEnd="@+id/editNombreEstado"
        android:layout_marginTop="37dp"
        android:onClick="limpiarTexto"/>

    <Button
        android:id="@+id/mainbttomarfotodetarecl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/adjuntarfoto"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/button16"
        android:layout_toLeftOf="@+id/button16" />

</RelativeLayout>
