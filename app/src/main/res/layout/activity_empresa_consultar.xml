<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.irvandoval.reclamosgrupo17.empresa.EmpresaInsertarActivity">

    <TextView android:text="@string/empresa_idempresa" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:id="@+id/textView26"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/idEmpresa"
        android:layout_alignBottom="@+id/textView26"
        android:layout_toRightOf="@+id/textView26"
        android:layout_toEndOf="@+id/textView26"
        android:layout_marginLeft="46dp"
        android:layout_marginStart="46dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/empresa_nombreempresa"
        android:id="@+id/textView27"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/nombreEmpresa"
        android:layout_alignBottom="@+id/textView27"
        android:layout_toEndOf="@+id/textView27"
        android:layout_toRightOf="@+id/textView27" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/consultar_boton"
        android:id="@+id/button13"
        android:onClick="consultarEmpresa"
        android:layout_marginTop="38dp"
        android:layout_below="@+id/textView26"
        android:layout_toLeftOf="@+id/nombreEmpresa"
        android:layout_toStartOf="@+id/nombreEmpresa" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/limpiar_boton"
        android:id="@+id/button14"
        android:onClick="limpiarTexto"
        android:layout_alignTop="@+id/button13"
        android:layout_alignRight="@+id/idEmpresa"
        android:layout_alignEnd="@+id/idEmpresa" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/empresa_idcategoriaempresa"
        android:id="@+id/textView49"
        android:layout_alignBottom="@+id/idCategoriaEmp"
        android:layout_toLeftOf="@+id/nombreEmpresa"
        android:layout_toStartOf="@+id/nombreEmpresa" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/idCategoriaEmp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_above="@+id/nombreEmpresa"
        android:layout_toRightOf="@+id/textView26"
        android:layout_toEndOf="@+id/textView26" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/empresa_cantidadsucursales"
        android:id="@+id/textView30"
        android:layout_below="@+id/textView27"
        android:layout_toLeftOf="@+id/button14"
        android:layout_toStartOf="@+id/button14"
        android:layout_marginTop="47dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/cantidadSucursales"
        android:layout_alignBottom="@+id/textView30"
        android:layout_toRightOf="@+id/textView30"
        android:layout_alignRight="@+id/nombreEmpresa"
        android:layout_alignEnd="@+id/nombreEmpresa" />

</RelativeLayout>
